class Solution:
    def addBinary(self, a: str, b: str) -> str:
        res=""
        carry=0
        a,b=a[::-1],b[::-1]
        for i in range(max(len(a),len(b))):
            dig1=ord(a[i])-ord("0") if i<len(a) else 0
            dig2=ord(b[i])-ord("0") if i<len(b) else 0 
            total=dig1+dig2+carry
            carry=total//2
            c=str(total%2)
            res=c+res
        if carry:
            return "1"+res 
        return res       
              
